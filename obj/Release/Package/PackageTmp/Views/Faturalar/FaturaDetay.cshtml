@using MVC5ONLINETICARIOTAMASYON.Models.Sınıflar;
@model List<FaturaKlaem>
@{
    ViewBag.Title = "FaturaDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />

<h2>Fatura Detayları</h2>
<br />
<ol class="breadcrumb mb-4">

    <li class="breadcrumb-item active"> </li>
</ol>
<br />
<table class="table table-hover">
    <tr>


        <th> Açıklama</th>
        <th> Miktar</th>
        <th> Brim Fiyat</th>
        <th> Tutar</th>


    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Aciklama</td>
            <td>@x.Miktar</td>
            <td>@x.BrimFiyat</td>
            <td>@x.Tutar</td>
            
        </tr>


    }

</table>
<a  href="/Faturalar/KalemEkle" class="btn btn-info">Yeni Kalem</a>